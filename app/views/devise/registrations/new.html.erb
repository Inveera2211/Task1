<br><br>
<br><br><br><br>

<div class="container">
  <br><br><br><br><br><br>
  <div class="row">

      
      
        

    <div class="col-xs-4">
 &nbsp;</div>
      <div class="col-xs-4"  style="background-color: white;
         box-shadow: inset 1px -1px 1px #994, inset -1px 1px 5px #994;">
<div class="form-group">
<%= render "devise/shared/links" %>



       <h4>Create Account</h4>
        
       
        
       <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>
        
        
       <div class="field" >
        <%= f.label :Email %><br>
            <%= f.email_field :email, autofocus: true %>
       </div>
      

       <div class="field">
         <%= f.label :password %>
          <% if @validatable %>
  
               
             <% end %><br/>
 
           <%= f.password_field :password, autocomplete: "off" %>
        </div>

<em>(<%= @minimum_password_length %> characters minimum)</em>
   <div class="field" >

    <td> <%= f.label :password_confirmation  %></td><br>
     <td><%= f.password_field :password_confirmation, autocomplete: "off" %></td>
   </div>
  <br>
  <div class="row">
    <div class="col-xs-4">
      <div class="actions">
       
    




<%= f.submit "SignUp",class:"btn btn-primary"%>

<% end %>

</div>
<div class="col-xs-8"> 
</div>

<br><br>
</div>
</div>
<div class="col-xs-5">&nbsp;</div>
</div>
</div>


